<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Recall">

    <ProgressBar
        android:id="@+id/progressBar"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/recallInstructions" />

    <ImageView
        android:id="@+id/symbol5"
        android:layout_width="104dp"
        android:layout_height="108dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toStartOf="@+id/symbol6"
        app:layout_constraintTop_toBottomOf="@+id/symbol2"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/symbol2"
        android:layout_width="104dp"
        android:layout_height="108dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toStartOf="@+id/symbol3"
        app:layout_constraintTop_toBottomOf="@+id/symbolToGuess"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/symbol3"
        android:layout_width="104dp"
        android:layout_height="108dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/symbolToGuess"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/symbol6"
        android:layout_width="104dp"
        android:layout_height="108dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/symbol3"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/symbol4"
        android:layout_width="104dp"
        android:layout_height="108dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/symbol1"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/symbol7"
        android:layout_width="104dp"
        android:layout_height="108dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/symbol4"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/symbol8"
        android:layout_width="104dp"
        android:layout_height="108dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toStartOf="@+id/symbol9"
        app:layout_constraintTop_toBottomOf="@+id/symbol5"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/symbol9"
        android:layout_width="104dp"
        android:layout_height="108dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/symbol6"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/symbol1"
        android:layout_width="104dp"
        android:layout_height="108dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/symbolToGuess"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/symbolToGuess"
        android:layout_width="104dp"
        android:layout_height="108dp"
        android:layout_marginStart="152dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/recallInstructions"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/recallInstructions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/oswald"
        android:gravity="center"
        android:text="What was the location of the symbol?"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>